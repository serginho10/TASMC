<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vivanco.tasmc.HotelesDisponibles">


    <include
        android:id="@+id/app_bar_hotel"
        layout="@layout/app_bar_hotel" />

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeHoteles"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/listHoteles"
            android:layout_below="@id/app_bar_hotel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </android.support.v4.widget.SwipeRefreshLayout>

</RelativeLayout>
