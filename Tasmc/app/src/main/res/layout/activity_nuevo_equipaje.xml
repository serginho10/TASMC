<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vivanco.tasmc.NuevoEquipaje">

    <include
        android:id="@+id/app_bar_equi"
        layout="@layout/app_bar_eq" />

    <TextView
        android:id="@+id/nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/app_bar_equi"
        android:layout_marginTop="26dp"
        android:text="@string/nombre" />

    <EditText
        android:id="@+id/equipaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/nombre"
        android:layout_toEndOf="@+id/nombre"
        android:hint="@string/hint_obligatorio" />

    <TextView
        android:id="@+id/icono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/nombre"
        android:layout_marginTop="45dp"
        android:text="@string/icono" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/icono" />

    <ImageView
        android:id="@+id/guarda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/app_bar_equi"
        android:layout_alignEnd="@+id/equipaje"
        android:src="@drawable/saveb" />

</RelativeLayout>
